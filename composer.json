{
    "name": "devmi/easy-socialite",
    "description": "Easy socialite setup for laravel",
    "type": "library",
    "keywords": [
        "laravel",
        "socialite",
        "easy-socialite"
    ],
    "authors": [
        {
            "name": "Mohamed Miloudi",
            "email": "milou0104@gmail.com",
            "homepage": "https://miloudi.io",
            "role": "Developer"
        },
        {
            "name": "Bedrani Sidali",
            "email": "bedrani.sidali.94@gmail.com",
            "homepage": "https://github.com/bboysidou",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "^5.7",
        "php": "^7.1",
        "laravel/socialite": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "orchestra/testbench": "^3.7",
        "doctrine/dbal": "^2.9"
    },
    "autoload": {
        "psr-4": {
